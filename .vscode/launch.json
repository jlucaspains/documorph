    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceRoot}/src/documorph.cli/bin/Debug/net8.0/documorph.cli.dll",
            "args": ["md", "--in", "d:/documorph/test/documorph.tests/data/docconverter.docx", "--out", "d:/documorph/test/documorph.tests/data/docconverter.md"],
            "cwd": "${workspaceRoot}",
            "externalConsole": false,
            "stopAtEntry": false,
            "internalConsoleOptions": "openOnSessionStart"
        }
    ]
}